    "raw-loader": "4.0.2",
    "replace-ext": "2.0.0",
    "sass": "1.43.4",
    "sass-loader": "12.3.0",
    "sass-rem": "3.0.0",
    "style-loader": "3.3.1",
    "stylelint": "14.1.0",
    "stylelint-config-sass-guidelines": "9.0.1",
    "terser-webpack-plugin": "5.2.5",
    "typescript": "4.4.4",
    "webpack": "5.64.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.5.0"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/elenabravo/webpack5-pug-sass-starter"
